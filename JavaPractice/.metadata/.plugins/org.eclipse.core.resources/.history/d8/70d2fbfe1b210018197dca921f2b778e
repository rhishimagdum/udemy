	// Search the element
	// Return result
	
	public int binarySearch(int[] numbers, int numberToSearch) {
		// Implement sorting logic
		// Bubble Sort algo
		BubbleSortAlgorithm bubbleSortAlgorithm = new BubbleSortAlgorithm();
